{
  "manifest_version": 3,
  "name": "Data Extractor",
  "version": "1.0",
  "description": "Extracts all website data to plaintext and saves it.",
  "permissions": ["activeTab", "scripting", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}

  